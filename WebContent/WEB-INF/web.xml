  </servlet-mapping>
  
  <servlet>
  	<servlet-name>register</servlet-name>
  	<servlet-class>com.yajie.servlet.Register</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>register</servlet-name>
  	<url-pattern>/User/register</url-pattern>
  </servlet-mapping>
  
